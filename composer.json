{
    "prefer-stable" : true,
    "require" : {
        "symfony/yaml" : "^3.3",
        "erusev/parsedown" : "^1.6",
        "symfony/filesystem" : "^3.3",
        "twig/twig" : "^2.4",
        "intervention/image" : "^2.4",
        "tightenco/collect" : "^5.5"
    },
    "require-dev" : {
        "larapack/dd" : "^1.0",
        "phpunit/phpunit" : "^6.0",
        "spatie/phpunit-watcher" : "^1.3"
    },
    "autoload" : {
        "psr-4" : {
            "Stitcher\\" : "src/Stitcher",
            "Pageon\\" : "src/Pageon"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Stitcher\\Test\\" : "tests",
            "Pageon\\Test\\" : "tests"
        }
    }
}
